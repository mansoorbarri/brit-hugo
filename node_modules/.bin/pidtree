#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/anar/Music/anar/gh/brit-hugo/node_modules/pidtree/bin/node_modules:/home/anar/Music/anar/gh/brit-hugo/node_modules/pidtree/node_modules:/home/anar/Music/anar/gh/brit-hugo/node_modules:/home/anar/Music/anar/gh/node_modules:/home/anar/Music/anar/node_modules:/home/anar/Music/node_modules:/home/anar/node_modules:/home/node_modules:/node_modules:/home/anar/Music/anar/gh/brit-hugo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/anar/Music/anar/gh/brit-hugo/node_modules/pidtree/bin/node_modules:/home/anar/Music/anar/gh/brit-hugo/node_modules/pidtree/node_modules:/home/anar/Music/anar/gh/brit-hugo/node_modules:/home/anar/Music/anar/gh/node_modules:/home/anar/Music/anar/node_modules:/home/anar/Music/node_modules:/home/anar/node_modules:/home/node_modules:/node_modules:/home/anar/Music/anar/gh/brit-hugo/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../pidtree/bin/pidtree.js" "$@"
else
  exec node  "$basedir/../pidtree/bin/pidtree.js" "$@"
fi
